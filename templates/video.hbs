{{>header}}
<div id="videos" class="container p-2">
  <div class="p-5"></div>
  <h1 class="pagetitle">video</h1>

  {{#each items}}
  <div class="row py-3">
    <div class="col-md-4">
      <iframe src="{{this.videoUrl}}" frameborder="0" allow="autoplay; encrypted-media;" allowfullscreen></iframe>
    </div>
      <div class="col-md-6">
          <h5 class="mt-0">{{this.title}}</h5>
          <p>{{this.description}}</p>
      </div>
  </div>
  {{/each}}
</div>
{{>footer}}