{{>header}}
<div id="audios" class="container py-2">
  <div class="py-5"></div>
  <h1 class="pagetitle">audio</h1>

  <div class="row">
    {{#each items}}
    <div class="media py-3">
      <iframe scrolling="no" frameborder="no" allow="autoplay"
        src="{{this.audioUrl}}"></iframe>
      <div class="card-body">
        <div class="media-body">
          <h5 class="mt-0">{{this.title}}</h5>
          <p>{{this.description}}</p>
        </div>
      </div>
    </div>
    {{/each}}

