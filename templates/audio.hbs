{{>header}}
<div class="py-5"></div>
{{#if audios}}
<div id="audios" class="container py-2">
  <h1 class="pagetitle">audio</h1>
  {{#each audios}}
  <div class="row py-3">
    <div class="col-md-4">
      <div class="embed-responsive embed-responsive-16by9">
        <iframe scrolling="no" frameborder="no" allow="autoplay" src="{{this.audioUrl}}"></iframe>
        <div class="spinner-border" role="status">
          <span class="sr-only">Loading...</span>
        </div>
      </div>
    </div>
    <div class="col-md-6">
      <h5>{{this.title}}</h5>
      <p>{{this.description}}</p>
    </div>
  </div>
  {{/each}}
</div>
{{else}}
<div id="audios" class="container py-2 h-75">
  <h1 class="pagetitle">audio</h1>

  <h4 class="py-2">No audios found. Please check back later.</h4>
</div>
{{/if}}
{{>footer}}
